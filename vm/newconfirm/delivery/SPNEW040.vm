                                          <td nowrap class="FontSmallBold" align="right">
                                                #customSpringMessage('CHECK_DELIV_DATE')<!--確認納期-->
                                          </td>
                                          <td nowrap class="FontSmallBold" align="left">
                                                #springFormInput("searchCondition.delivCheckDateFrom.dateOfymdFormat" "size=12 maxLength=8 class='Small noIME' onblur='dateOnBlur(this);' onfocus='dateOnFocus(this);' ")
                                                <img class="Icon" src="../../image/btnCalendar.gif" onclick="initSearchBox(1,getObj('delivCheckDateFrom.dateOfymdFormat'));">
                                                &nbsp;～&nbsp;
                                                #springFormInput("searchCondition.delivCheckDateTo.dateOfymdFormat" "size=12 maxLength=8 class='Small noIME' onblur='dateOnBlur(this);' onfocus='dateOnFocus(this);' ")
