<!doctype html>
<html lang="ja">
<head>
<title>#springMessage('ACCEPT_DETAIL_DTL')</title>
<meta http-equiv="Content-type" content="text/html; charset=UTF-8">
<meta http-equiv="Pragma" content="no-cache">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta http-equiv="Content-script-type" content="text/javascript">
<link id="CSS_FONT" rel="stylesheet" href="../../include/FontS.css" type="text/css">
<link rel="stylesheet" href="../../include/Layout.css" type="text/css">
<link rel="stylesheet" href="../../include/Color.css" type="text/css">
<link rel="stylesheet" href="../../include/searchbox.css" type="text/css">
<script language="javascript" type="text/javascript" src="../../include/common.js"></script>
<script language="javascript" type="text/javascript" src="../../include/check.js"></script>
<script language="javascript" type="text/javascript" src="../../include/searchbox.js"></script>
<script language="javascript" type="text/javascript" src="../../include/date.js"></script>
<script language="javascript" type="text/javascript" src="../../include/jquery-1.7.2.min.js"></script>
<script language="javascript" type="text/javascript">
<!--
#parse("./common/common_js.vm")
//-->
</script>
</head>
<body class="DetailBody" onload="getObj('printButton').focus();">
    <FORM NAME="Body">
    <table class="NoBorder" width="95%"  nowrap>
    <tr align="left">
        <br>
        <u><span id="StepTitle" class="FontEDItitle">
            <script type="text/javascript">document.write(document.title);</script>
        </span></u>
    </tr>
    </table>
    <table class="NoBorder" style="width:100%;">
        <tr><td align="left">#customSpringMessage('SPACP012_HEADER_COMMENT')</td></tr>
        <tr>
            <td class="DTL_MSGBOX" align="right" nowrap>
                <input id="printButton" type="button" class="MiddleButton" value="#springMessage('PRINT')" onClick="window.print();">
                <input type="button" class="MiddleButton" value="#springMessage('CLOSE')" onClick="window.close();">
            </td>
        </tr>
    </table>
    <table class="NoBorder" style="width:100%;">
        <tr>
            <td class="NoBorder" height="auto">
                <table class="Box1G" width="100%" align="center">
                                <tr id="Header">
                                    <td class="Box1H FontBoxHeader" nowrap>#customSpringMessage('ORDER_NO')<br>#customSpringMessage('DELIV_KEY_NO')</td>
                                    <td class="Box1H FontBoxHeader" nowrap>#customSpringMessage('REVISION_NUMBER')</td>
                                    <td class="Box1H FontBoxHeader" nowrap>#customSpringMessage('REQ_TYPE_IND')<br>#customSpringMessage('CORRECT_NAME')</td>
                                    <td class="Box1H FontBoxHeader" nowrap>#customSpringMessage('ACCEPT_DATE')</td>
                                    <td class="Box1H FontBoxHeader" nowrap>#customSpringMessage('ITEM_NAME')<br>#customSpringMessage('ITEM_CD')<br>#customSpringMessage('KATASHIKI')</td>
                                    <td class="Box1H FontBoxHeader" nowrap>#customSpringMessage('ACCEPT_VOL')</td>
                                    <td class="Box1H FontBoxHeader" nowrap>#customSpringMessage('UNIT')</td>
                                    <td class="Box1H FontBoxHeader" nowrap>#customSpringMessage('CURRENCY_CD')<br>#customSpringMessage('TAX_IND')<br>#customSpringMessage('TAX_RATE')</td>
                                    <td class="Box1H FontBoxHeader" nowrap>#customSpringMessage('ACCEPT_AMOUNT')</td>
                                    #if($!acceptConsumptAmountDispInd == '1')
                                    <td class="Box1H FontBoxHeader" nowrap>#customSpringMessage('CONSUMPT_AMOUNT')</td>
                                    <td class="Box1H FontBoxHeader" nowrap>#customSpringMessage('TOTAL_AMOUNT')</td>
                                    #end
                                    <td class="Box1H FontBoxHeader" nowrap>#customSpringMessage('ORDER_USER')<br>#customSpringMessage('CONNTACT')<br>#customSpringMessage('MAIL_ADDR')</td>
                                    <td class="Box1H FontBoxHeader" nowrap>#customSpringMessage('PAY_CONDITION')</td>
                                </tr>
                            #foreach($!deItem in $!detailItem.acceptanceItemInfoDetailItemList)
                                #if("$!deItem" != "")
                                <tr>
                                    <td class="Box1D FontSlipNo" nowrap>#out($!deItem.orderNo.orderNo)<br>#out($!deItem.delivKeyNo)</td>
                                    <td class="Box1D FontMiniAnk" nowrap>#out($!deItem.historyNo.historyNo)</td>
                                    <td class="Box1D FontMiniAnk #correctIndColor($deItem.correctInd)" nowrap>#out($!deItem.reqInd.reqInd_PageValue)<br>#out($!deItem.correctInd.correctInd_PageValue)</td>
                                    <td class="Box1D FontMiniAnk" nowrap>#out($!deItem.acceptDate.dateOfymdFormat)</td>
                                    <td class="Box1D FontMiniAnk">#out($!deItem.item.itemName)<br>#out($!deItem.item.itemCd)<br>#out($!deItem.item.katashiki)</td>
                                    <td class="Box1Dr" nowrap>#out($!deItem.acceptVol.volFormat)</td>
                                    <td class="Box1D FontMiniAnk" nowrap>#out($!deItem.unit)</td>
                                    <td class="Box1D FontMiniAnk" nowrap>#out($!deItem.currencyCd)<br>#out($!deItem.taxInd_PageValue)#if($!deItem.taxRate != '')<br>#out($!deItem.taxRate)&nbsp;%#end</td>
                                    #if($!deItem.acceptAmount.amountMarkDispForSP)
                                        <td class="Box1Dr FontCurrSmall"  nowrap>
                                    #else
                                        <td class="Box1D FontCurrSmall"  nowrap>
                                    #end
                                        #out($!deItem.acceptAmount.amountMarkDispForSP)
                                    </td>
                                    #if($!acceptConsumptAmountDispInd == '1')
                                        #if($!deItem.totalAmount.amountMarkDispForSP)
                                        <td class="Box1Dr FontCurrSmall"  nowrap>
                                        #else
                                        <td class="Box1D FontCurrSmall"  nowrap>
                                        #end
                                        #out($!deItem.consumptAmount.amountMarkDispForSP)
                                    </td>

                                        #if($!deItem.totalAmount.amountMarkDispForSP)
                                        <td class="Box1Dr FontCurrSmall"  nowrap>
                                        #else
                                        <td class="Box1D FontCurrSmall"  nowrap>
                                        #end
                                        #out($!deItem.totalAmount.amountMarkDispForSP)
                                    </td>
                                    #end
                                    <td class="Box1D FontMiniAnk" >
                                        #out($!deItem.custInfo.custUser.userName)<br>
                                        #out($!deItem.custInfo.custUser.contact.telNo.telNo)<br>
                                        <a id = "link.$!emailAddr" class="FontMiniAnk" href='mailto:$!deItem.custInfo.custUser.userName<$!deItem.custInfo.custUser.contact.mailAddr.mail>'>
                                            #out($!deItem.custInfo.custUser.contact.mailAddr.mail)</a>
                                    </td>
                                    <td class="Box1D FontMiniAnk" nowrap>
                                        #out($!deItem.payConditionItem.payConditionName)
                                    </td>
                            </tr>
                                #end
                            #end

                </table>
            </td>
        </tr>
        <tr><td align="left">#customSpringMessage('SPACP012_FOOTER_COMMENT')</td></tr>
    </table>
<br>
    </FORM>
</body>

</html>