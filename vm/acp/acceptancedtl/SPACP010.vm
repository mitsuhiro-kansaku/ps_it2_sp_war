</script>
</head>
<body  class="BodyArea" onload="onLoad();">
<form  id="BodyForm" name="Body" method="post">
    #customFormHiddenInput("searchCondition.page.pageRecorders.hidden" "$!defaultSearchCondition.searchCondition.page.pageRecorders")
    #customFormHiddenInput("searchObject.searchObject.hidden" "$!defaultSearchCondition.searchObject.searchObject")
    <iframe id="CALENDARBOX" class="CALENDARBOX" framespacing="0"scrolling="no" frameborder="0" src="../../sbxCalendar/body.vm"></iframe>
    <table class="NoBorder" style="width:100%;height:100%;z-index:0;">
        <tr>
            <td id="SEARCH_AREA" class="SearchArea"><!-- onLoadで変更 -->
                #parse("./common/common_title_nonew.vm")
                <table width="100%">
                    <tr id="SEARCH_AREA2">
                        <td  id="Adjust" align="right" Valign="top">
                            <!-- 検索条件入力部 -->
                            <table class="NoBorder" width="100%">
                                <tr>
                                    <td align="left" colspan="4" nowrap>
                                        #springMessage('SPACP010_HEADER_COMMENT')
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <!--検索条件表示エリア-->
                                        <table class="NoBorder" width="100%">
                                            <tr>
                                                <td nowrap class="FontSmallBold" align="right">
                                                    #customSpringMessage('FISCAL_YEAR_MONTH')
                                                </td>
                                                <td nowrap class="FontSmallBold" align="left">
                                                    #springFormInput("searchCondition.fiscalYearMonth.dateOfymFormat" "class='Small noIME' size='12' type='text' maxlength='6'
                                                        onblur='dateOnBlurYYYYMM(this);'
                                                        onfocus='dateOnFocus(this);'")
                                                </td>
                                                <td nowrap class="FontSmallBold" align="right">
                                                </td>
                                                <td nowrap class="FontSmallBold" align="left">
                                                </td>
                                            </tr>
                                        </table>
                                    </td>
                                    <!-- 行数を変更する場合はROWSPANに注意！ -->
                                    <td nowrap class="FontSmallBold" align="right" ROWSPAN="4" align="right" Valign="top">
                                        <br>
                                        <input type="button" class="LargeButton" value="#springMessage('ACTION_SEARCH')" onclick="javascript:execSearch();">
                                        <br><br>
                                           <a href="javascript:clear();">#customSpringMessage('ACTION_CLEAR_SEARCH_CRITERIA')</a>&nbsp;
                                    </td>
                                </tr>
                                <tr>
                                    <td align="left" colspan="4" nowrap class="FontComment">
                                        #customSpringMessage("SPACP010_FOOTER_COMMENT")
                                    </td>
                                </tr>
                            </table>
                        </td>
                    </tr>
                </table>
            </td>
        </tr>
        <tr id="UPLOADAREA">
            <td class="UploadArea" class="NoBorder" style="height:35px;border-bottom:1px solid #999999;text-align:left;vertical-align:top;">
                <table class="NoBorder" width="100%" style="margin:auto;">
                    <tr>
                        <td align="center">
                        </td>
                        <td align="right">
                            <input class="SmallButton" type="button" value="#customSpringMessage('ACCEPT_DETAIL_SUMMARY_DOWNLOAD')" onclick="javascript:doSumDownload()">
                            <input class="SmallButton" type="button" value="#customSpringMessage('ACCEPT_DETAIL_DATA_DOWNLOAD')" onclick="javascript:doDownload()">
                        </td>
                    </tr>
                </table>
            </td>
        </tr>
        <tr id="TR_ERROR" style="display: none;">
            <td class="error" style="height:0px;">
                <div id="ERRORDIV">$!errorMessage </div>
            </td>
        </tr>
        <tr style="vertical-align:top;height:100%;">
            <td class="NoBorder" style="height:100%;">
                <iframe id="DATAAREA" name="DATAAREA" class="DataArea" framespacing="0" frameborder="0" src="../../layout/default.vm" scrolling="no" frameborder="no"></iframe>
            </td>
        </tr>
</table>
<iframe id="HIDDENAREA" name="HIDDENAREA" framespacing="0" frameborder="0" src="" scrolling="no" frameborder="no" style="display:none"></iframe>
</FORM>
</BODY>
</html>
