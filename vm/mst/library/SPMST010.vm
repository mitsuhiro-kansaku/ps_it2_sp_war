<!doctype html>
<html lang="ja">
<head>
<meta http-equiv="Content-type" content="text/html; charset=UTF-8">
<title>#customSpringMessage('SCREEN_M_LIBRALY_LIST')</title><!-- ライブラリ -->
<meta http-equiv="Pragma" content="no-cache">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta http-equiv="Content-script-type" content="text/javascript">
<link rel="stylesheet" href="../../include/searchbox.css" type="text/css">
<link id="CSS_FONT" rel="stylesheet" href="../../include/FontS.css" type="text/css">
<link rel="stylesheet" href="../../include/Layout.css" type="text/css">
<link rel="stylesheet" href="../../include/Color.css" type="text/css">
<link rel="stylesheet" href="../../include/ime.css" type="text/css">
<script language="javascript" type="text/javascript" src="../../include/jquery-1.7.2.min.js"></script>
<script language="javascript" type="text/javascript" src="../../include/common.js"></script>
<script language="javascript" type="text/javascript" src="../../include/searchbox.js"></script>
<script language="javascript" type="text/javascript" src="../../include/date.js"></script>
<script language="javascript" type="text/javascript" src="../../include/check.js"></script>
<script language="javascript" type="text/javascript">
<!--
var pinName="MstLibrary010_Pin";
#set($commonTitle = "#customSpringMessage('SCREEN_M_COND_LIST')")
#parse("./common/common_js.vm")
function onLoad() {
    bodyLoad();
    var alertMessage = "$!alertMessage";
    if (alertMessage != ""){
        alert(alertMessage);
    }
    var errorMsg = "$!errorMessage";

    if (errorMsg != ""){
        getObj("TR_ERROR").style.display = "";
        setErrorMessage("ERRORDIV", errorMsg, "DATAAREA");
        return;
    }
    var isShowList = "#jsEscape($isShowList)";
    if (isShowList == 'true'){
        iframeChange("${flowExecutionUrl}&_eventId=search");
    }
}
function execSearch(){
    buttonDisable();
    getObj('BodyForm').action = "${flowExecutionUrl}&_eventId=validate";
    getObj('BodyForm').submit();
}
//-->
</script>
</head>
<body class="BodyArea" onload="onLoad();">
<form id="BodyForm" name="Body" action="${flowExecutionUrl}&_eventId=validate" method="post">
	<iframe id="CALENDARBOX" class="CALENDARBOX" framespacing="0"scrolling="no" frameborder="0" src="../../sbxCalendar/body.vm"></iframe>
    <table class="NoBorder" style="width:100%;height:100%;z-index:0;">
        <tr>
            <td id="SEARCH_AREA" class="SearchArea"><!-- onLoadで変更 -->
                #parse("./common/common_title_nonew.vm")
                <table width="100%">
                    <tr id="SEARCH_AREA2">
                        <td id="Adjust" style="text-align:left;">
                            <!-- 検索条件入力部 -->
                            <table class="NoBorder" width="100%">
                                <tr>
                                    <td align="left" colspan="4" nowrap>
                                        #customSpringMessage('SPMST010_HEADER_COMMENT')
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <!--検索条件表示エリア-->
                                        <table class="NoBorder" width="100%">
                                            <tr>
                                                <td nowrap class="FontSmallBold" align="right">
                                                    #customSpringMessage('LIBRALY_TITLE_NAME')<span class="FontComment">％</span><!-- ライブラリ件名 -->
                                                </td>
                                                <td nowrap class="FontSmallBold" align="left">
                                                    #springFormInput("searchCondition.titleName" "size=60 maxLength=30 class='Small onIME'")
                                                </td>
                                                <td nowrap class="FontSmallBold" align="right">
                                                    #customSpringMessage('REG_DATE')<!-- 登録日時 -->
                                                </td>
                                                <td nowrap class="FontSmallBold" align="left">
                                                    #springFormInput("searchCondition.regDateFrom.dateOfymdFormat" "maxLength=8 size=12 class='MiddleAnk noIME' onblur='dateOnBlur(this);' onfocus='dateOnFocus(this);'  ")
                                                    <img class="Icon" src="../../image/btnCalendar.gif" onclick="initSearchBox(1,getObj('regDateFrom.dateOfymdFormat'));">
                                                    &nbsp;～&nbsp;
                                                    #springFormInput("searchCondition.regDateTo.dateOfymdFormat" "maxLength=8 size=12 class='MiddleAnk noIME' onblur='dateOnBlur(this);' onfocus='dateOnFocus(this);'  ")
                                                    <img class="Icon" src="../../image/btnCalendar.gif" onclick="initSearchBox(1,getObj('regDateTo.dateOfymdFormat'));">
                                                </td>
                                            </tr>
                                            <tr>
                                                <td nowrap class="FontSmallBold" align="right">
                                                    #customSpringMessage('DESCRIPTION')<span class="FontComment">％</span><!--  -->
                                                </td>
                                                <td nowrap class="FontSmallBold" align="left">
                                                    #springFormInput("searchCondition.description" "size=60 maxLength=200 class='Small onIME'")
                                                </td>
                                                <td nowrap class="FontSmallBold" align="right">
                                                    #customSpringMessage('FILE_NAME')<span class="FontComment">＊</span>
                                                </td>
                                                <td nowrap class="FontSmallBold" align="left">
                                                    #springFormInput("searchCondition.fileName" "size=60 maxLength=64 class='Small onIME'")
                                                </td>
                                            </tr>
                                            <tr>
                                                <td align="left" colspan="4" nowrap class="FontComment">
                                                    #customSpringMessage("SPMST010_FOOTER_COMMENT") ## ＊:前方一致 / ％:中間一致 / なし:完全一致
                                                </td>
                                            </tr>
                                        </table>
                                    </td>
                                        <!--検索ボタンエリア-->
                                    <td class="FontSmallBold" align="right">
                                        <br>
                                        <input type="button" onclick="javascript:execSearch();" class="LargeButton" value="#customSpringMessage('ACTION_SEARCH')"><!--検索-->
                                        <br>
                                        <br>
                                           <a href="javascript:clear();">#customSpringMessage('ACTION_CLEAR_SEARCH_CRITERIA')</a>&nbsp;<!-- 検索条件をクリアする -->
                                    </td>
                                </tr>
                            </table>
                            <!-- 検索条件入力部 -->
                        </td>
                    </tr>
                </table>
            </td>
        </tr>
        <tr id="TR_ERROR" style="display: none;">
            <td class="error" style="height:0px;">
                <div id="ERRORDIV">$!errorMessage</div>
            </td>
        </tr>
        <tr style="vertical-align:top;height:100%;">
            <td class="NoBorder" style="height:100%;">
                <iframe id="DATAAREA" name="DATAAREA" class="DataArea" framespacing="0" frameborder="0" src="../../layout/default.vm" scrolling="no" frameborder="no"></iframe>
            </td>
        </tr>
    </table>

<!--検索部テーブル終了-->
<iframe id="HIDDENAREA" name="HIDDENAREA" framespacing="0" frameborder="0" src="" scrolling="no" frameborder="no" style="display:none"></iframe>
</form>

</body>
</html>
