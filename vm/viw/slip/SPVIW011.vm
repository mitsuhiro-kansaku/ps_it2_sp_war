            ## 2013.01.09 DEL DAIKO_YAMAGUCHI -END-
            <td class="BoxDetailLeft FontList">#out($!orderItem.buyerCompany)</td>
            <td class="BoxDetailCenter FontList">#out($!orderItem.orderDate.dateOfymdFormat)</td>
            <td class="BoxDetailCenter FontList">#out($!orderItem.changeOrderDate.dateOfymdFormat)</td>
            <td class="BoxDetailLeft FontList">#out($!orderItem.itemName)</td>
            <td class="BoxDetailLeft FontList">#out($!orderItem.itemCd)</td>
            <td class="BoxDetailLeft FontList" nowrap>#out($!orderItem.katashiki)</td>
