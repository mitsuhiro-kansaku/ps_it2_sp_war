            ##<td class="BoxDetailRight FontList2" nowrap>#out($!orderItem.orderVol.volFormat)</td>
            ## +++++ 2013/03/24 ADD DAIKO_TAKEDA
            <td class="BoxDetailRight FontList2" nowrap>#out($!orderItem.originVol.volFormat)</td>
            <td class="BoxDetailRight FontList2" nowrap>#out($!orderItem.orderRemainAmount.volFormat)</td>
            ## +++++ 2013/03/24 ADD DAIKO_TAKEDA
            <td class="BoxDetailRight FontList2" nowrap>#out($!orderItem.orderCancelVol.volFormat)</td>
            <td class="BoxDetailCenter FontList">#out($!orderItem.unit)</td>
            <td class="BoxDetailCenter FontList">#out($!orderItem.orderDelivDate.dateOfymdFormat)</td>
            <td class="BoxDetailCenter FontList">#out($!orderItem.currencyCd)</td>
