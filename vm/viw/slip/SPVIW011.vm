            #if ($!orderItem.unitPrice.moneyDisp)
            <td class="BoxDetailRight FontCurrSmall" nowrap>
            #else
            <td class="BoxDetailCenter FontCurrSmall" nowrap>
            #end
                #out($!orderItem.unitPrice.amountMarkDispForSP)
                #if($!orderItem.unitPriceInd == '0')
