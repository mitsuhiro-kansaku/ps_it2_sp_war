                #out($!orderItem.originAmount.amountMarkDispForSP)
                ## 2013.03.29 DEL DAIKO_WANG START
                #*
                #if($!orderItem.unitPriceInd == '0')
                    <BR><font class="FontMini" color=red>【#customSpringMessage('PROV_PRICE')】</font>
                #end
                *#
                ## 2013.03.29 DEL DAIKO_WANG -END-
            </td>
            #else
            <td class="BoxDetailCenter FontCurrSmall" nowrap>
                ## ----- 2013/03/24 DEL DAIKO_TAKEDA
                ## #out($!orderItem.utlAmount.amountMarkDispForSP)
                ## +++++ 2013/03/24 ADD DAIKO_TAKEDA
                #out($!orderItem.originAmount.amountMarkDispForSP)
            </td>
            #end
                #if($!orderItem.orderCancelAmount.moneyDisp)
                <td class="BoxDetailRight FontCurrSmall">
                #else
                <td class="BoxDetailCenter FontCurrSmall">
                #end
                    #out($!orderItem.orderCancelAmount.amountMarkDispForSP)
                </td>
                #if($!reqUserDispInd == "1" || $!reqUserDispInd == "3")
                <td class="BoxDetailLeft FontList">#out($!orderItem.reqSectionPlaceName)</td>
                <td class="BoxDetailLeft FontList">#out($!orderItem.reqSectionName)</td>
                <td class="BoxDetailLeft FontList">#out($!orderItem.reqUserName)</td>
                <td class="BoxDetailLeft FontList">#out($!orderItem.reqTelNo)</td>
                #end
            #if ($!SPVIW011Cooperation.isCooperation())
                #foreach($!cooperationConfig in $SPVIW011Cooperation.Configs)
            <td class="$!SPVIW011Cooperation.getCooperationClass($!cooperationConfig)">
                    #if ($!cooperationConfig.itemType != "8")
                        #out($!SPVIW011Cooperation.getCooperationItem($!orderItem, $!cooperationConfig))
                    #else
                        $!SPVIW011Cooperation.getCooperationItem($!orderItem, $!cooperationConfig)
                    #end
            </td>
                #end
            #end
            </tr>
        #end
    </tbody>
</table>
<table id="WAIT" class="NoBorder" style="display:none;width:100%;">
    <tr>
        <td style="text-align:center;">
            <br><br>
            <img src="../../image/wait.gif">
            <br>
