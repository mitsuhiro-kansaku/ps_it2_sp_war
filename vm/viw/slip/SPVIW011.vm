            </th>
            <th label="case" class="BoxHeaderCenter FontSmall" id="S_ORDER.CURRENCY_CD" onclick="parent.iframeChange('${flowExecutionUrl}&_eventId=order','column','S_ORDER.CURRENCY_CD')">
                #customSpringMessage('CURRENCY_CD') <!-- 通貨コード  -->
            </th>
            ## 2013.03.29 ADD DAIKO_WANG START
            <th class="BoxHeaderCenter FontSmall" id="S_ORDER.UNIT_PRICE">
                #customSpringMessage('ORDER_UNIT_PRICE') ## 注文単価
            </th>
            ## 2013.03.29 ADD DAIKO_WANG -END-
            ## ----- 2013/03/22 DEL DAIKO_TAKEDA START
            #*
            <th label="num" class="BoxHeaderCenter FontSmall" id="S_ORDER.ORDER_AMOUNT" onclick="parent.iframeChange('${flowExecutionUrl}&_eventId=order','column','S_ORDER.ORDER_AMOUNT')">
                #customSpringMessage('ORDER_AMOUNT') <!-- 注文金額  -->
            </th>
