            tmp2.value=tmp2.value.replace(/,/g,"");
        var tmp3=document.getElementById("quotRespInfoPriceManagement.quotRespInputItemPriceList["+i+"].lotList.lot3.unitPrice.amount.amountFormat");
            tmp3.value=tmp3.value.replace(/,/g,"");
        var tmp4=document.getElementById("quotRespInfoPriceManagement.quotRespInputItemPriceList["+i+"].lotList.lot4.unitPrice.amount.amountFormat");
            tmp4.value=tmp4.value.replace(/,/g,"");
        var tmp5=document.getElementById("quotRespInfoPriceManagement.quotRespInputItemPriceList["+i+"].lotList.lot5.unitPrice.amount.amountFormat");
            tmp5.value=tmp5.value.replace(/,/g,"");
     }
}
function convertToComma(){
     var mount=#jsEscape($!{quotRespPrice.quotDetail.quotItemList.size()});
     for(var i = 0; i < mount; i++){
         var tmp1=document.getElementById("quotRespInfoPriceManagement.quotRespInputItemPriceList["+i+"].lotList.lot1.unitPrice.amount.amountFormat");
