#parse("./common/common_js.vm")
function onLoad() {
    setFocus();
    var alertMessage = "$!alertMessage";
    if (alertMessage != ""){
        alert(alertMessage);
    }
    var errorMsg = "$!errorMessage";
    if (errorMsg == ""){
          var isAnswer = '#jsEscape($!isAnswer)';
          if(isAnswer == 'true'){
               #set( $mQuotReply = "#customSpringMessage('QUOT_REPLY')" )
                     alert('#customMessageArgs("I005",[$mQuotReply])');
                     buttonDisable();
                     top.iframeChange("${flowExecutionUrl}&_eventId=back");
               }
                var isDismiss = '#jsEscape($!isDismiss)';
                if(isDismiss == 'true'){
                   #set( $mQuotReply = "#customSpringMessage('QUOT_REPLY')" )
                      alert('#customMessageArgs("I033",[$mQuotReply])');
                      buttonDisable();
                      top.iframeChange("${flowExecutionUrl}&_eventId=back");
                 }
    }
    convertToComma();
}

function exec() {
     #set( $mmQuotReply = "#customSpringMessage('QUOT_REQ')" )
     #set( $reply = "#customSpringMessage('REQ_REPLY')" )
     if( !confirm('#customMessageArgs("W001",[$mmQuotReply,$reply])') ) {
          return;
     }
     setFileDeleteFlg();
     ## +++++ 強化-00041-000 2013/04/03 ADD SBO_KANGJW START +++++
     buttonDisable();
     ## +++++ 強化-00041-000 2013/04/03 ADD SBO_KANGJW END +++++
     getObj("Body").action="${flowExecutionUrl}&_eventId=answer";
     getObj("Body").submit();
}

function dismiss(){
    #set( $mmQuotReply = "#customSpringMessage('QUOT_REQ')" )
    #set( $refuse = "#customSpringMessage('REFUSE')" )
    if( !confirm('#customMessageArgs("W001",[$mmQuotReply,$refuse])') ) {
        return;
    }
    setFileDeleteFlg();
    ## +++++ 強化-00041-000 2013/04/03 ADD SBO_KANGJW START +++++
    buttonDisable();
    ## +++++ 強化-00041-000 2013/04/03 ADD SBO_KANGJW END +++++
    getObj("Body").action="${flowExecutionUrl}&_eventId=dismiss";
    getObj("Body").submit();
}

function clearfile(no) {
    var obj = document.getElementById("file"+no);
    var str = obj.innerHTML;
    obj.innerHTML = str;
}
function setFileDeleteFlg() {
     var mount=#jsEscape($!{quotRespPrice.quotDetail.quotItemList.size()});
     for(var i = 0; i < mount; i++){
        var del = getObj("quotRespInfoPriceManagement.quotRespInputItemPriceList["+i+"].attachFile.screenDelFlg").value;
        var file = getObj("quotRespInfoPriceManagement.quotRespInputItemPriceList["+i+"].attachFile.screenFile").value;
        if("1" == del && file.length > 0) {
            getObj("quotRespInfoPriceManagement.quotRespInputItemPriceList["+i+"].attachFile.screenDelFlg").value = "0";
        }

     }
}

function downloadFile(index) {
  parent.iframeChange('${flowExecutionUrl}&_eventId=download','index',index);
}

 function convert(){
    //delete ','
    var mount=#jsEscape($!{quotRespPrice.quotDetail.quotItemList.size()});
    for(var i = 0; i < mount; i++){
        var tmp1=document.getElementById("quotRespInfoPriceManagement.quotRespInputItemPriceList["+i+"].lotList.lot1.unitPrice.amount.amountFormat");
         	tmp1.value=tmp1.value.replace(/,/g,"");
