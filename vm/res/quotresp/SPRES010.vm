<script language="javascript" type="text/javascript">
<!--
var pinName="ResQuotresp_Pin";
#parse("./common/common_js.vm")

function onLoad(){
    bodyLoad();
    var alertMessage = "$!alertMessage";
    if (alertMessage != ""){
        alert(alertMessage);
    }
    var errorMsg = "$!errorMessage";

    if (errorMsg != ""){
        getObj("TR_ERROR").style.display = "";
        setErrorMessage("ERRORDIV", errorMsg, "DATAAREA");
        return;
    }
    var isDownload = "#jsEscape($!isDownLoadValidate)";
    if (isDownload == 'true') {
        iframeChange("${flowExecutionUrl}&_eventId=download","pageid","SPRES020","HIDDENAREA");
    } else {
        var isShowList = "#jsEscape($isShowList)";
        var isShowInfo = "#jsEscape($!isShowInfo)";
        var isQuickLink = '#jsEscape($isQuickLink)';

		if (isShowInfo == 'true') {
			top.iframeChange("${flowExecutionUrl}&_eventId=showUploadResult");
		} else {

            if (isShowInfo == 'false') {
            	parentButtonEnable();
                #set( $mQutoResp = "#customSpringMessage('QUOT_REPLY')" )
                alert("#customMessageArgs('I005',[$mQutoResp])");

            }

            if (isShowList == 'true'){
                if(isShowInfo == 'false' && self.frameElement.getAttribute('name') == "HIDDENAREA"){
                    parent.iframeChange("${flowExecutionUrl}&_eventId=search","pageid","SPRES020");
                }else{
                    iframeChange("${flowExecutionUrl}&_eventId=search","pageid","SPRES020");
                }
            }
            else if (isQuickLink == 'true'){
                if(isShowInfo == 'false' && self.frameElement.getAttribute('name') == "HIDDENAREA"){
                    parent.iframeChange("${flowExecutionUrl}&_eventId=search","pageid","SPRES020","isQuickLink","true");
                }else{
                    iframeChange("${flowExecutionUrl}&_eventId=search","pageid","SPRES020","isQuickLink","true");
                }
            }
		}
    }
}

function execSearch() {
    getObj("BodyForm").target="_self";
    buttonDisable();
    getObj("BodyForm").action = "${flowExecutionUrl}&_eventId=validate";
    getObj('BodyForm').submit();
}
function doDownload() {
    //メッセージは空設定
    getObj("ERRORDIV").innerHTML="";
    buttonDisable();
    setTimeout(buttonUsable,1000);
    getObj("BodyForm").target = "HIDDENAREA";
    getObj("BodyForm").action = "${flowExecutionUrl}&_eventId=downloadValidate";

    getObj("BodyForm").submit();
}

function doUpload(){
    if(getObj('file').value!=""){
        #set( $mQutoResp = "#customSpringMessage('QUOT_REPLY')" )
        #set( $mUpload = "#customSpringMessage('ACTION_UPLOAD')" )
        if( !confirm('#customMessageArgs("W001",[$mQutoResp,$mUpload])'))  {
           return;
        }
        buttonDisable();
        getObj("BodyForm").target="_self";
        getObj('BodyForm').action = "${flowExecutionUrl}&_eventId=upload";
        getObj("BodyForm").submit();
    }else{
        getObj("TR_ERROR").style.display = "";
        document.getElementById("ERRORDIV").innerHTML="E130 #customSpringMessage('E130')";
        document.getElementById("DATAAREA").contentWindow.document.body.innerText = "";
    }
}

function clearfile() {
	var obj = document.getElementById("file1");
	var str = obj.innerHTML;
	obj.innerHTML = str;
}

//-->
</script>
</head>
<body  class="BodyArea"  onload="onLoad();">
<form  id="BodyForm" name="Body"  method="post" enctype="multipart/form-data">
    <iframe id="CALENDARBOX" class="CALENDARBOX" framespacing="0"scrolling="no" frameborder="0" src="../../sbxCalendar/body.vm"></iframe>
    #customFormHiddenInput("searchCondition.page.pageRecorders.hidden" "$!defaultSearchCondition.searchCondition.page.pageRecorders")
    #customFormHiddenInput("ansFlg.hidden" "$!defaultSearchCondition.ansFlg")
    <table class="NoBorder" style="width:100%;height:100%;z-index:0;">
        <tr>
            <td id="SEARCH_AREA" class="SearchArea"><!-- onLoadで変更 -->
                #parse("./common/common_title_nonew.vm")
                <table width="100%">
                    <tr id="SEARCH_AREA2">
                        <td  id="Adjust" align="right" Valign="top">
                            <!-- 検索条件入力部 -->
                             <table class="NoBorder" width="100%">
                                <tr>
                                    <td align="left" colspan="4" nowrap>
                                        #customSpringMessage('SPRES010_HEADER_COMMENT')
                                    </td>
                                </tr>
                               <tr>
                                <td>
                                  <table class="NoBorder" width="100%">
                                    <tr>
                                        <td nowrap class="FontSmallBold" align="right">
                                            #customSpringMessage('QUOT_REQ_NO')<span class="FontComment">＊</span><!--見積依頼番号-->
                                        </td>
                                        <td nowrap class="FontSmallBold" align="left">
                                            #springFormInput("searchCondition.aplNo" "size=40 maxLength=19 class='Small noIME'")
                                        </td>
                                        <td nowrap class="FontSmallBold" align="right">
                                             #customSpringMessage('QUOT_ANS_LIMIT')<!--回答期限-->
                                        </td>
                                        <td nowrap class="FontSmallBold" align="left">
                                            #springFormInput("searchCondition.quotRespLimitDateFrom.dateOfymdFormat" "size=12 maxLength=8 class='Small noIME' onblur='dateOnBlur(this);' onfocus='dateOnFocus(this);' ")
                                            <img class="Icon" src="../../image/btnCalendar.gif" onclick="initSearchBox(1,getObj('quotRespLimitDateFrom.dateOfymdFormat'));">
                                            &nbsp;～&nbsp;
                                            #springFormInput("searchCondition.quotRespLimitDateTo.dateOfymdFormat" "size=12 maxLength=8 class='Small noIME' onblur='dateOnBlur(this);' onfocus='dateOnFocus(this);' ")
                                            <img class="Icon" src="../../image/btnCalendar.gif" onclick="initSearchBox(1,getObj('quotRespLimitDateTo.dateOfymdFormat'));">
                                        </td>
                                    </tr>
                                    <tr>
                                        <td nowrap class="FontSmallBold" align="right">
                                            #customSpringMessage('QUOT_REQ_DATE')<!--見積依頼日-->
                                        </td>
                                        <td nowrap class="FontSmallBold" align="left">
                                            #springFormInput("searchCondition.quotReqDateFrom.dateOfymdFormat" "size=12 maxLength=8 class='Small noIME' onblur='dateOnBlur(this);' onfocus='dateOnFocus(this);' ")
                                            <img class="Icon" src="../../image/btnCalendar.gif" onclick="initSearchBox(1,getObj('quotReqDateFrom.dateOfymdFormat'));">
                                            &nbsp;～&nbsp;
                                            #springFormInput("searchCondition.quotReqDateTo.dateOfymdFormat" "size=12 maxLength=8 class='Small noIME' onblur='dateOnBlur(this);' onfocus='dateOnFocus(this);' ")
                                            <img class="Icon" src="../../image/btnCalendar.gif" onclick="initSearchBox(1,getObj('quotReqDateTo.dateOfymdFormat'));">
                                        </td>
                                        <td nowrap class="FontSmallBold" align="right">
                                        </td>
                                        <td nowrap class="FontSmallBold" align="left">
                                        </td>
                                    </tr>
                                    <tr>
                                        <td nowrap class="FontSmallBold" align="right">
                                            #customSpringMessage('DISPLY_OBJECT')<!--表示対象-->
                                        </td>
                                        <td nowrap class="FontSmall" align="left">
                                            <div id="ListShow">
                                                #customFormRadioButtons("searchCondition.ansFlg" $ANS_FLG "" "" "")
                                            </div>
                                        </td>
                                        <td nowrap class="FontSmallBold" align="right">
                                        </td>
                                        <td nowrap class="FontSmallBold" align="left">
                                        </td>
                                    </tr>
                                    <tr>
                                        <td align="left" colspan="4" nowrap class="FontComment">
                                            #customSpringMessage('SPRES010_FOOTER_COMMENT')</span>
                                        </td>
                                    </tr>
                                 </table>
                                 </td>
                                   <!-- 行数を変更する場合はROWSPANに注意！ -->
                                 <td nowrap class="FontSmallBold" align="right" ROWSPAN="3" align="right" Valign="top">
                                     <br>
                                     <input type="button" class="LargeButton" value="#customSpringMessage('ACTION_SEARCH')" onclick="javascript:execSearch();">
                                        <br><br>
                                     <a href="javascript:clear();">#customSpringMessage('ACTION_CLEAR_SEARCH_CRITERIA')</a>&nbsp;
                                 </td>
                                </tr>
                            </table>
                        </td>
                    </tr>
                </table>
            </td>
        </tr>
        <tr>
            <td class="UploadArea" class="NoBorder" style="height:35px;border-bottom:1px solid #999999;text-align:left;vertical-align:top;">
                #parse("./common/common_upload_download_clear.vm")
            </td>
        </tr>
        <tr id="TR_ERROR" style="display: none;">
