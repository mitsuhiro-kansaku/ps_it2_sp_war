		document.getElementById("CSS_LAYOUT").href = "../include/layoutL.css";
	} else {
		document.Body.FontSize[1].checked = true;
	}
*/

	top.SLIPNO = "";

	changeFontSize0();
	getObj('BODYAREA').contentWindow.changeFontSize();


}

function openConfig() {
	closeMenu();

	setDisplay("CMDUSERON0",true);
	setDisplay("CMDUSERON1",true);
	setDisplay("CMDUSEROFF",false);



}

function closeConfig() {
	setDisplay("CMDUSERON0",false);
	setDisplay("CMDUSERON1",false);
	setDisplay("CMDUSEROFF",true);
}

function changeFontSize( size ) {
	setCookie("FONTSIZE",size);

	changeFontSize0(size);

	getObj('BODYAREA').contentWindow.changeFontSize(size);

}


function changeRole() {
	setText("ROLENAME",top.ROLES[document.Body.Roles.selectedIndex][1]);
	// ＨＯＭＥのときは、再表示。
	try {
		parent.getObj('BODYAREA').contentWindow.getObj('DATAAREA').contentWindow.reDraw(top.ROLES[document.Body.Roles.selectedIndex][2]);
	} catch(e) {
	}
}

