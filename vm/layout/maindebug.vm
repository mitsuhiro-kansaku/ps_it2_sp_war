function onLoad() {
createMenu();
document.Body.a.value = menustr;

	for(var i=0; i<document.styleSheets.length; i++) {
		if( document.styleSheets[i].href == "../include/default.css" ) {
			CSS_DEFAULT = i;
		} else if( document.styleSheets[i].href == "../include/Layout.css" ) {
			CSS_LAYOUT = i;
		}
	}

	setDisplay("CMDUSERON0",false);
	setDisplay("CMDUSERON1",false);

	if( getCookie("FONTSIZE")=="L" ) {
		document.Body.FontSize[0].checked = true;
	} else if( getCookie("FONTSIZE")=="M" ) {
		document.Body.FontSize[1].checked = true;
	} else {
		document.Body.FontSize[2].checked = true;
	}

	// ロール初期設定
	var i;
	document.Body.Roles.length = top.ROLES.length;
	for(i=0; i<top.ROLES.length; i++) {
		document.Body.Roles.options[i].value = 0;
		document.Body.Roles.options[i].text = top.ROLES[i][0]+" （"+top.ROLES[i][1]+"）";
	}


