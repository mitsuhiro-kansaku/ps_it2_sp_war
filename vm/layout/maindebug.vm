var MENUOPEN = false,id=0,MENUCNT=0,MAXMENUNO=5,OPENLOADING=false;
var ALARM = new Array(0,1,1,0,0,1,0,0);
var menustr = "";

function write2(wstr) {
	menustr = menustr + wstr;
}

function createMenu() {
	var w,i,cnt=0;
	write2('<TABLE CLASS="NoBorder"><TR>');
	write2('<TD STYLE="width:85px;text-aling:center;height:28px;white-space:nowrap;" BACKGROUND="../image/picMenu0.gif">&nbsp;</TD>');
	for(i=0; i<MenuList.length; i++) {
		if( getIndentNo(MenuList[i])==0 ) {
			write2('<TD STYLE="text-aling:center;vertical-align:top;padding-top:3px;height:28px;white-space:nowrap;" ID="MENU'); write2(cnt); write2('"');
			if( ALARM[cnt]>0 ) {
				write2(' BACKGROUND="../image/picMenu1_.gif"');
			} else {
				write2(' BACKGROUND="../image/picMenu1.gif"');
			}

			write2('>');
//			if( cnt==0 ) {
				//write2('<SPAN CLASS="MenuFont">&nbsp;&nbsp;</SPAN>');
//			}

			w = MenuList[i].split("___");
			write2('<SPAN CLASS="MenuFont">');
			if( w[0]=="お気に入り" ) {
				write2('<IMG src="../image/picStar.gif" ALIGN="left">');
			}
			write2('<A ID="MENUSTR_'); write2(cnt+'" ONMOUSEOVER="this.style.backgroundColor=\'plum\';if( MENUOPEN ) {openMenu(this,0,'+cnt+');}" ONMOUSEOUT="if( !MENUOPEN ) {this.style.backgroundColor=\'transparent\'}" ONCLICK="if( MENUOPEN ) { ');
				if( w.length > 1 ) {
					write2('changePage(\''+w[1]+'\');');
				} else {
					write2('closeMenu();');
				}

			write2('} else { ');
				if( w.length > 1 ) {
					write2('changePage(\''+w[1]+'\');');
				} else {
					write2('openMenu(this,0,'+cnt+'); ');
				}
			write2('}">');

			write2("&nbsp;"+w[0]+"&nbsp;");
			write2('</A>');
			//write2('<SPAN CLASS="MenuFont">&nbsp;&nbsp;&nbsp;</SPAN>');
			write2('</SPAN>');
			write2('</TD>');
			cnt++;
		}
	}
	write2('<TD STYLE="width:20px;text-aling:center;height:28px;white-space:nowrap;" BACKGROUND="../image/picMenu2.gif">&nbsp;</TD>');
	write2('</TR></TABLE>');
	MENUCNT = cnt;
	for(i=0; i<=MAXMENUNO; i++) { // １つ多くとる。
		write2('<TABLE ID="MENUWINDOW'+i+'" CLASS="NoBorder" STYLE="position:absolute;left:0px;z-index:' +(1000+i)+';filter:progid:DXImageTransform.Microsoft.DropShadow(color=#666666,offX=4,offY=4,positive=1);"><TBODY STYLE="display:none;" ID="LEVEL'+ i + '"></TBODY></TABLE>');
	}
}

function openMenu(obj,level,topmenuNo,childNo) {

	closeConfig();

	var h,i,j,w,wStr;
	var tr,td,span;

	if( OPENLOADING ) {
		return;
	}

	if( level==0 ) {
		for(i=0; i<MAXMENUNO; i++) {
			setDisplay("LEVEL"+i,false);
		}
		for(i=0; i<MENUCNT; i++) {
			if( i==topmenuNo ) {
				getObj("MENUSTR_"+i).style.backgroundColor = "plum";
			} else {
				getObj("MENUSTR_"+i).style.backgroundColor = "transparent";
			}
		}
	} else {
		w = obj.id.lastIndexOf("_");
		getObj(obj.id.substring(0,w)).style.backgroundColor = "plum";
	}

	MENUOPEN = true;
	for(i=getObj("LEVEL"+level).childNodes.length-1; i>=0; i--) {
		getObj("LEVEL"+level).removeChild(getObj("LEVEL"+level).childNodes.item(i));
	}
	for(i=level+1; i<MAXMENUNO; i++) {
		setDisplay("LEVEL"+i,false);
	}
	var childno=0;
	var menustr = trim(getText2(obj));
	if( (w=menustr.toUpperCase().indexOf("<IMG"))>=0 ) {
		menustr = menustr.substring(0,w);
	}
	var wMenuLevel = new Array(MAXMENUNO);
	var wIndent = -1;
	for(i=0; i<MAXMENUNO; i++) {
		wMenuLevel[i] = -1;
	}

	var wNo = -1;
	for(i=0; i<MenuList.length; i++) {
		if( getIndentNo(MenuList[i])<wNo ) {
			for(j=getIndentNo(MenuList[i])+1; j<MAXMENUNO; j++) {
				wMenuLevel[j] = -1;
			}
		}
		wNo = getIndentNo(MenuList[i]);
		wMenuLevel[getIndentNo(MenuList[i])]++;
		if( getIndentNo(MenuList[i])==(level+1) && getMenuItem(MenuList[i],level).split("___")[0]==menustr ) {
			var wIdElements = obj.id.replace("MENUSTR_","").split("_");
			var wFlg = true;
			for(j=0; j<=level; j++) {
				//alert(j+":"+wMenuLevel[j]+":"+wIdElements[j]);
				if( wMenuLevel[j] != eval(wIdElements[j]) ) {
					wFlg = false;
					break;
				}
			}
			if( !wFlg ) {
				continue;
			}
			td = document.createElement("TD");
			td.id = obj.id + "_" + (childno++);
			td.style.width = "auto";
			td.style.height = "auto";
			td.onmouseout = function() { this.style.backgroundColor = "#FFFFFF"; }
			td.onmouseover = function() {
				var obj = this.parentNode.parentNode;
				for(i=0; i<obj.childNodes.length; i++) {
					obj.childNodes.item(i).childNodes(0).style.backgroundColor = "#FFFFFF";
				}
				this.style.backgroundColor = "plum";
				openMenu(this,level+1);
			}
			w = getMenuItem(MenuList[i],level+1).split("___");
			if( isIE() ) {
				setText2(td,w[0]);
			} else {
				td.innerHTML = w[0];
			}
			if( i<MenuList.length-1 && getIndentNo(MenuList[i+1])==level+2 ) {
				img = document.createElement("IMG");
				img.src = "../image/mrkNextmenu.gif";
				td.insertBefore(img,null);
			}

			if( w.length>1 ) {
				td.className = "MenuList";
				td.setAttribute("url",w[1]);
				td.onclick = function() { changePage(this.getAttribute("url")); }
			} else {
				td.className = "MenuList2";
			}
			tr = document.createElement("TR");
			tr.insertBefore(td,null);
			if( level>=MAXMENUNO-1 ) {
				alert("これ以上メニューを表示できません。");
				return;
			}
			getObj("LEVEL"+level).insertBefore(tr,null);
			setDisplay("LEVEL"+level,true);
		}
	}

	var x = 88,y = 0;
	if( level== 0 ) {
		for(i=0;i<topmenuNo; i++) {
			x += eval(getObj("MENU"+i).offsetWidth);
		}
		y = 61;
	} else {
		x = eval(getObj("MENUWINDOW"+(level-1)).offsetLeft);
		x += eval(getObj("MENUWINDOW"+(level-1)).offsetWidth);
		x -= 10;
		y = eval(getObj("MENUWINDOW"+(level-1)).offsetTop);
		w = obj.id.split("_");
		w = eval(w[w.length-1]);
		for(i=0; i<eval(w); i++) {
			y += eval(obj.offsetHeight);
		}
		y += 6;
	}
	getObj("MENUWINDOW"+level).style.left = x;
	getObj("MENUWINDOW"+level).style.top = y;

	OPENLOADING = false;

}

function closeMenu() {
	var i;
	for(i=0; i<MAXMENUNO; i++) {
		setDisplay("LEVEL"+i,false);
	}
	for(i=0; i<MENUCNT; i++) {
		getObj("MENUSTR_"+i).style.backgroundColor = "transparent";
	}
	MENUOPEN = false;
}

function getIndentNo(data) {
	return data.split("|||").length - 1;
}
function getMenuItem(data,level) {
	if( level<0 || level>MAXMENUNO ) {
		return "";
	}
	var w = data.split("|||");
	if( w.length > level ) {
		return w[level];
	} else {
		return "";
	}
}





