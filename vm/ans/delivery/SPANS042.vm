<!doctype html>
<html lang="ja">
<head>
#set($commonTitle = "#customSpringMessage('DELIV_RCV_DETAIL')")
<title>$commonTitle</title><!--納期回答詳細-->
<meta http-equiv="Content-type" content="text/html; charset=UTF-8">
<meta http-equiv="Pragma" content="no-cache">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta http-equiv="Content-script-type" content="text/javascript">
<link id="CSS_FONT" rel="stylesheet" href="../../include/FontS.css" type="text/css">
<link rel="stylesheet" href="../../include/Layout.css" type="text/css">
<link rel="stylesheet" href="../../include/Color.css" type="text/css">
<link rel="stylesheet" href="../../include/searchbox.css" type="text/css">
<script language="javascript" TYPE="text/javascript" src="../../include/common.js"></script>
<script language="javascript" TYPE="text/javascript" src="../../include/check.js"></script>
<script language="javascript" TYPE="text/javascript" src="../../include/date.js"></script>
<script language="javascript" type="text/javascript" src="../../include/jquery-1.7.2.min.js"></script>
<script language="javascript" TYPE="text/javascript">
<!--
## ビューが初めて表示される直前に発生します。
function onLoad(){
    getObj('printButton').focus();
}
//-->
</script>
</head>
<body class="DetailBody" onload="onLoad();">
<table class="NoBorder" width="95%">
    <tr>
        <td>
            <span id="StepTitle" class="FontEDItitle">
                $commonTitle
            </span>
        </td>
    </tr>
</table>
<form name="BodyForm" method="post">
    <table class="NoBorder" style="width:100%;">
        <tr>
            <td align="left">
                #customSpringMessage('SPANS042_HEADER_COMMENT')
            </td>
        </tr>
        <tr>
            <td class="DTL_MSGBOX" align="right" nowrap>
                <input id="printButton" type="button" class="MiddleButton" value="#customSpringMessage('PRINT')" onClick="window.print();">
                <input type="button" class="MiddleButton" value="#customSpringMessage('CLOSE')" onClick="window.close();">
            </td>
        </tr>
    </table>
    <p class="FontVender">
        #out($!delivDetail.orderBaseInfo.accAbbrName)<!--受注者名-->
    </p>
    <table class="Box1G" width="760px">
        <tbody class="Box1G">
            <tr>
                <td class="Box1H FontBoxHeader" nowrap>
                    #customSpringMessage('ORDER_NO')<!--注文番号-->
                </td>
                <td class="Box1D FontSlipNo" colspan="2">
                    #out($!delivDetail.delivRespIdentify.orderNo)
                </td>
            </tr>
            <tr>
                <td class="Box1H FontBoxHeader" nowrap>
                    #customSpringMessage('DELIV_RCV_DATE')<!--納期回答日-->
                </td>
                <td class="Box1D FontSmallAnk" colspan="2">
                    #out($delivDetail.delivInfoList.delivCheckDate.dateOfymdFormat)
                </td>
            </tr>
            <tr>
                <td class="Box1H FontBoxHeader" nowrap>
                    #customSpringMessage('ORDER_VOL')<!--注文数量-->
                </td>
                <td class="Box1D FontSmallAnk" colspan="2">
                    #out($!delivDetail.orderBaseInfo.originVol.volFormat)
                </td>
            </tr>
            <tr>
                <td class="Box1H FontBoxHeader" nowrap>
                    #customSpringMessage('ORDER_DELIV_DATE')<!--注文納期-->
                </td>
                <td class="Box1D FontSmallAnk" colspan="2">
                    #out($!delivDetail.orderBaseInfo.orderDeliv.dateOfymdFormat)
                </td>
            </tr>
            <tr>
                <td class="Box1H FontBoxHeader" nowrap>
                    #customSpringMessage('RCV_DETAIL')<!--回答備考-->
                </td>
                <td class="Box1D FontSmallAnk" colspan="2">
                    #out($!delivDetail.delivInfoList.checkDetail)
                </td>
            </tr>
            <tr>
                <td class="Box1H FontBoxHeader" nowrap>
                    #customSpringMessage('CONFIRM_MARK')<!--確認マーク-->
                </td>
                <td class="Box1D FontSmallAnk" colspan="2">
                    #out($!delivDetail.delivInfoList.confirmMark_PageValue)
                </td>
            </tr>
        </tbody>
    </table>
    <br>
    <table class="Box1G" width="760px">
        <thead>
            <tr>
                <td nowrap class="Box1H FontBoxHeader">
                    #customSpringMessage('PART_DELIV_NO')<!--分納回数-->
                </td>
                <td nowrap class="Box1H FontBoxHeader">
                    #customSpringMessage('QUOT_RESP_DELIV_DATE')<!--回答納期-->
                </td>
                <td nowrap class="Box1H FontBoxHeader">
                    #customSpringMessage('QUOT_RESP_VOL')<!--回答数量-->
                </td>
            </tr>
        </thead>
        <tbody>
        #foreach($!item in $!delivDetail.delivInfoList.delivInfoItemList)
            <tr>
                <td class="Box1D FontSmallAnk" style="text-align:center;">
                    $foreach.count<!--分納回数-->
                </td>
                <td class="Box1D FontSmallAnk" style="text-align:center;">
                    #out($!item.checkDelivDate.dateOfymdFormat)<!--回答納期-->
                </td>
                <td class="Box1D FontSmallAnk" style="text-align:center;">
                    #out($!item.checkAmount.volFormat)<!--回答数量-->
                </td>
            </tr>
        #end
        </tbody>
    </table>
    <table class="NoBorder" style="width:100%;">
        <tr>
            <td align="left">
                #customSpringMessage('SPANS042_FOOTER_COMMENT')
            </td>
        </tr>
    </table>
<br>
</form>
</body>
</html>