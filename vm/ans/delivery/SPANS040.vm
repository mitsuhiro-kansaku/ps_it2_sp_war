<!doctype html>
<html lang="ja">
<head>
#if("#jsEscape($!isQuickLink)" == 'true')
    #set($commonTitle = "#customSpringMessage('DELIV_DATE_COLLECTIVE_REPLY')(#out($!searchCondition.delivDateRespListType_PageValue))")
#else
    #set($commonTitle = "#customSpringMessage('DELIV_DATE_COLLECTIVE_REPLY')")
#end
<title>$commonTitle</title><!--納期一括回答-->
<meta http-equiv="Content-type" content="text/html; charset=UTF-8">
<meta http-equiv="Pragma" content="no-cache">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta http-equiv="Content-script-type" content="text/javascript">
<link id="CSS_FONT" rel="stylesheet" href="../../include/FontS.css" type="text/css">
<link rel="stylesheet" href="../../include/Layout.css" type="text/css">
<link rel="stylesheet" href="../../include/Color.css" type="text/css">
<link rel="stylesheet" href="../../include/searchbox.css" type="text/css">
<link rel="stylesheet" href="../../include/ime.css" type="text/css">
<script language="javascript" type="text/javascript" src="../../include/common.js"></script>
<script language="javascript" type="text/javascript" src="../../include/check.js"></script>
<script language="javascript" type="text/javascript" src="../../include/date.js"></script>
<script language="javascript" type="text/javascript" src="../../include/searchbox.js"></script>
<script language="javascript" type="text/javascript" src="../../include/jquery-1.7.2.min.js"></script>
<script language="javascript" type="text/javascript">
<!--
#parse("./common/common_js.vm")
var pinName="SPANS040_Pin";
## ビューが初めて表示される直前に発生します。
function onLoad() {
    bodyLoad();
    var alertMessage="$!alertMessage";
    if(alertMessage!=""){
        alert(alertMessage);
    }
    var errorMsg="$!errorMessage";

    if(errorMsg!=""){
        getObj("TR_ERROR").style.display = "";
        setErrorMessage("ERRORDIV",errorMsg,"DATAAREA");
        return;
    }
    var isShowList="#jsEscape($isShowList)";
    if(isShowList=="true"){
        iframeChange("${flowExecutionUrl}&_eventId=search","pageid","SPANS041");
        return;
    }
    var isQuickLink="#jsEscape($!isQuickLink)";
    if(isQuickLink=="true"){
        iframeChange("${flowExecutionUrl}&_eventId=search","pageid","SPANS041","isQuickLink","true");
        return;
    }
}
## 「検索」ボタンをクリックしたとき発生します。
function execSearch() {
    buttonDisable();
    getObj("BodyForm").action = "${flowExecutionUrl}&_eventId=validate";
    getObj("BodyForm").submit();
}
## 「納期回答処理」ボタンをクリックしたとき発生します。
function doEdit(){
    var dataArea=document.getElementById('DATAAREA').contentWindow;
    if(dataArea!=null&&typeof(eval(dataArea.doEdit))!="undefined"){
        dataArea.doEdit();
    }
}
//-->
</script>
</head>
<body class="BodyArea" onload="onLoad();">
<form id="BodyForm" name="Body" method="post">
    <iframe id="CALENDARBOX" class="CALENDARBOX" framespacing="0"scrolling="no" frameborder="0" src="../../sbxCalendar/body.vm"></iframe>
    #customFormHiddenInput("pageid" "SPANS040")
    <table class="NoBorder" width="100%" height="100%">
        <tr>
            <td id="SEARCH_AREA" class="SearchArea"><!--onLoadで変更-->
                #parse("./common/common_title_nonew.vm")
                <table width="100%">
                    <tr id="SEARCH_AREA2">
                        <td id="Adjust">
                            <!-- 検索条件入力部 -->
                            <table class="NoBorder" width="100%">
                                <tr>
                                    <td nowrap align="left" colspan="4">
                                        #customSpringMessage('SPANS040_HEADER_COMMENT')
                                    </td>
                                </tr>
                                <tr>
                                    <td nowrap class="FontSmallBold" align="right">
                                        #customSpringMessage('ORDER_NO')<!--注文番号-->
                                        <span class="FontComment">＊</span>
                                    </td>
                                    <td nowrap class="FontSmallBold" align="left" colspan="3">
                                        #springFormInput("searchCondition.orderNo" 'class="Small noIME" maxlength="19" size="30"')
                                    </td>
                                </tr>
                                <tr>
                                    <td nowrap class="FontSmallBold" align="right">
                                        #customSpringMessage('ORDER_DELIV_DATE')<!--注文納期-->
                                    </td>
                                    <td nowrap class="FontSmallBold" align="left">
                                        #springFormInput("searchCondition.orderDelivDateFrom.dateOfymdFormat" 'class="Small noIME" maxlength="8" size="12" onblur="dateOnBlur(this);" onfocus="dateOnFocus(this);"')
                                        <img class="Icon" src="../../image/btnCalendar.gif" onclick="initSearchBox(1,getObj('orderDelivDateFrom.dateOfymdFormat'));">
                                        &nbsp;～&nbsp;
                                        #springFormInput("searchCondition.orderDelivDateTo.dateOfymdFormat" 'class="Small noIME" maxlength="8" size="12" onblur="dateOnBlur(this);" onfocus="dateOnFocus(this);"')
                                        <img class="Icon" src="../../image/btnCalendar.gif" onclick="initSearchBox(1,getObj('orderDelivDateTo.dateOfymdFormat'));">
                                    </td>
                                    <td nowrap class="FontSmallBold" align="right">
                                        #customSpringMessage('CHECK_DELIV_DATE')<!--確認納期-->
                                    </td>
                                    <td nowrap class="FontSmallBold" align="left">
                                        #springFormInput("searchCondition.checkDelivDateFrom.dateOfymdFormat" 'class="Small noIME" maxlength="8" size="12" onblur="dateOnBlur(this);" onfocus="dateOnFocus(this);"')
                                        <img class="Icon" src="../../image/btnCalendar.gif" onclick="initSearchBox(1,getObj('checkDelivDateFrom.dateOfymdFormat'));">
                                        &nbsp;～&nbsp;
                                        #springFormInput("searchCondition.checkDelivDateTo.dateOfymdFormat" 'class="Small noIME" maxlength="8" size="12" onblur="dateOnBlur(this);" onfocus="dateOnFocus(this);"')
                                        <img class="Icon" src="../../image/btnCalendar.gif" onclick="initSearchBox(1,getObj('checkDelivDateTo.dateOfymdFormat'));">
                                    </td>
                                </tr>
                                <tr>
                                    <td nowrap class="FontSmallBold" align="right">
                                        #customSpringMessage('DISPLY_OBJECT')<!--表示対象-->
                                    </td>
                                    <td nowrap class="FontSmall" align="left" colspan="3">
                                        #customFormRadioButtons("searchCondition.delivDateRespListType" $DELIV_DATE_RESP_LIST_TYPE "" "" "")
                                        #customFormHiddenInput("displayObject" "")
                                    </td>
                                </tr>
                                <tr>
                                    <td nowrap class="FontSmallBold" align="right">
                                    </td>
                                    <td nowrap class="FontSmall" align="left"><!--検収済を除く-->
                                        #customFormCheckbox("searchCondition.excludeAcceptedFlg" "class='FontSmallBold'" "1" "")#customSpringMessage('EXCLUDE_ACCEPTE')
                                    </td>
                                </tr>
                                <tr>
                                    <td nowrap class="FontComment" align="left" colspan="4">
                                        #customSpringMessage('SPANS040_FOOTER_COMMENT')<!--＊:前方一致 / ％:中間一致 / なし:完全一致-->
                                    </td>
                                </tr>
                            </table>
                            <!--検索条件入力部-->
                        </td>
                        <td nowrap class="FontSmallBold" align="right" valign="top">
                            <br>
                            <input type="button" onclick="javascript:execSearch();" class="LargeButton" value="#customSpringMessage('ACTION_SEARCH')"><!--検索-->
                            <br>
                            <br>
                            <a href="javascript:clear();javascript:clearCheckBoxHidden('excludeAcceptedFlg');">#customSpringMessage('ACTION_CLEAR_SEARCH_CRITERIA')<!--検索条件をクリアする--></a>&nbsp;
                        </td>
                    </tr>
                </table>
            </td>
        </tr>
        <tr id="UPLOAD_AREA">
            <td class="UploadArea">
                <table class="NoBorder" width="100%" style="margin:auto;">
                    <tr>
                        <td align="left">
                            <table width="100%">
                                <td align="left">
                                    <table>
                                        <tr>
                                            <td id="edit" style="display:none">
                                                <input id="button_exec_nm" class="SmallButton" type="button" value="#customSpringMessage("DELIV_DATE_RESP_ACTION")" onclick="doEdit();">
                                            </td>
                                        </tr>
                                    </table>
                                </td>
                                <td align="center"></td>
                                <td align="right"></td>
                            </table>
                        </td>
                    </tr>
                </table>
            </td>
        </tr>
        <tr id="TR_ERROR" style="display: none;">
            <td class="error" style="height:0px;">
                <div id="ERRORDIV">$!errorMessage</div>
            </td>
        </tr>
        <tr style="vertical-align:top;height:100%;">
            <td class="NoBorder" style="height:100%;">
                <iframe id="DATAAREA" name="DATAAREA" class="DataArea" framespacing="0" frameborder="0" src="../../layout/default.vm" scrolling="no" frameborder="no"></iframe>
            </td>
        </tr>
    </table>
<iframe id="HIDDENAREA" name="HIDDENAREA" framespacing="0" frameborder="0" src="" scrolling="no" frameborder="no" style="display:none"></iframe>
</form>
</body>
</html>
