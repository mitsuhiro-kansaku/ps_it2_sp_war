<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="sp.check.impl.exclusive.ShipmentExclusiveCheckMapper">

    <select id="searchExclusive" resultType="String">
        SELECT
            S_DELIVERY.ACC_CD
        FROM 
            S_DELIVERY
        WHERE
            S_DELIVERY.DELIVERY_SEQ = #{shipmentSeq}
        AND S_DELIVERY.LAST_UPD_DATE = #{lastUpdDate}
    </select>
    
</mapper>