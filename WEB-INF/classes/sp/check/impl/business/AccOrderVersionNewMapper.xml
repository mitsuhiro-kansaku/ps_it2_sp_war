<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="sp.check.impl.business.AccOrderVersionNewMapper">

    <select id="getMaxOrderVersion" resultType="String">
       SELECT MAX(ORDER_VERSION)
         FROM S_ORDER
        <where>
            ACC_CD =#{accCd}
            AND CUST_CD =#{custCd}
            AND ORDER_NO = #{orderNo}
        </where>   
    </select>    
</mapper>
