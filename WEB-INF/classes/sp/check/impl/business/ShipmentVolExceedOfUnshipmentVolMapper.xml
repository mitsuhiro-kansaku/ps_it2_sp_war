<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sp.check.impl.business.ShipmentVolExceedOfUnshipmentVolMapper">

    <!-- 最大分納回数を取得 -->
    <select id="getShipmentVolOld" parameterType="String" 
       resultType="String">
        SELECT 
            SHIPMENT_VOL
        FROM
            S_DELIVERY
        WHERE
            S_DELIVERY.DELIVERY_SEQ = #{shipmentSeqNo}
    </select>
    
</mapper>