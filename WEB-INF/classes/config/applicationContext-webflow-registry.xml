        <webflow:flow-location path="newconfirm/quotreq/SPNEW010.xml" id="newconfirm/quotReq/SPNEW010"/>
        
        <!-- 見積回答 -->
        <webflow:flow-location path="res/quotresp/SPRES010.xml" id="res/quotresp/SPRES010"/>
        <webflow:flow-location path="res/quotresp/SPRES020.xml" id="res/quotresp/SPRES020"/>
        <webflow:flow-location path="res/quotresp/SPRES030.xml" id="res/quotresp/SPRES030"/>
        <webflow:flow-location path="res/quotresp/SPRES030Input.xml" id="res/quotresp/SPRES030Input"/>
        
        <!-- 新着納期確認 -->
        <webflow:flow-location path="newconfirm/delivery/SPNEW040.xml" id="newconfirm/delivery/SPNEW040"/>
        <webflow:flow-location path="newconfirm/delivery/SPNEW041.xml" id="newconfirm/delivery/SPNEW041"/>
        <webflow:flow-location path="newconfirm/delivery/SPNEW042.xml" id="newconfirm/delivery/SPNEW042"/>
        
        <!-- 新着注文 -->
        <webflow:flow-location path="newconfirm/order/SPNEW030.xml" id="newconfirm/order/SPNEW030"/>
