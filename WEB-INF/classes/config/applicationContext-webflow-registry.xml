        <webflow:flow-location path="shp/shipping/SPSHP040.xml" id="shp/shipping/SPSHP040"/>
        
        <!-- 備考入力 -->
        <webflow:flow-location path="cmn/remark/SPCMN020.xml" id="cmn/remark/SPCMN020"/>
        
        <!-- 検収情報 -->
        <webflow:flow-location path="newconfirm/acceptance/SPNEW061.xml" id="newconfirm/acceptance/SPNEW061"/>
        <webflow:flow-location path="newconfirm/acceptance/SPNEW060.xml" id="newconfirm/acceptance/SPNEW060"/>
        <webflow:flow-location path="newconfirm/acceptance/SPNEW062.xml" id="newconfirm/acceptance/SPNEW062"/>
        <!-- 検収明細 -->
        <webflow:flow-location path="acp/acceptancedtl/SPACP010.xml" id="acp/acceptancedtl/SPACP010"/>
        <webflow:flow-location path="acp/acceptancedtl/SPACP011.xml" id="acp/acceptancedtl/SPACP011"/>
        <webflow:flow-location path="acp/acceptancedtl/SPACP012.xml" id="acp/acceptancedtl/SPACP012"/>
        
        <!-- ライブラリ一覧 -->
        <webflow:flow-location path="mst/library/SPMST010.xml" id="mst/library/SPMST010"/>
        <webflow:flow-location path="mst/library/SPMST020.xml" id="mst/library/SPMST020"/>
        
