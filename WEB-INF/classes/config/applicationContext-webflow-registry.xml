        <webflow:flow-location path="prc/prcrevans/SPPRC020.xml" id="prc/prcrevans/SPPRC020"/>
        
        <!-- 納期回答 -->
        <webflow:flow-location path="ans/delivery/SPANS010.xml" id="ans/delivery/SPANS010"/>
