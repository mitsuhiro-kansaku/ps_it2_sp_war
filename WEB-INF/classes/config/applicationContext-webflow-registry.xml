        <webflow:flow-location path="newconfirm/order/SPNEW032.xml" id="newconfirm/order/SPNEW032"/>
        
        <!-- 新着見積採否 -->
        <webflow:flow-location path="newconfirm/quotado/SPNEW022Other.xml" id="newconfirm/quotAdo/SPNEW022Other"/>
        <webflow:flow-location path="newconfirm/quotado/SPNEW022.xml" id="newconfirm/quotAdo/SPNEW022"/>
        <webflow:flow-location path="newconfirm/quotado/SPNEW021.xml" id="newconfirm/quotAdo/SPNEW021"/>
        <webflow:flow-location path="newconfirm/quotado/SPNEW020.xml" id="newconfirm/quotAdo/SPNEW020"/>
        
        <!-- 新着入荷情報 -->
        <webflow:flow-location path="newconfirm/arrival/SPNEW050.xml" id="newconfirm/arrival/SPNEW050"/>
        <webflow:flow-location path="newconfirm/arrival/SPNEW051.xml" id="newconfirm/arrival/SPNEW051"/>
        <webflow:flow-location path="newconfirm/arrival/SPNEW052.xml" id="newconfirm/arrival/SPNEW052"/>
        
