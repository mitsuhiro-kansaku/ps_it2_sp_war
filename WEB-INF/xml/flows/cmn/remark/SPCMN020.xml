<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow
        http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd"
	parent="base">

	<input name="shipmentManagement" />
	<input name="lineNo" />
	<input name="deliverySeq" />

	<view-state id="SPCMN020" view="cmn/remark/SPCMN020" model="shipmentManagement">
		<transition on="save" validate="true">
			<set name="flashScope.isSuccess" value="true"></set>
			<evaluate
				expression="SPPRT020Service.saveRemark(deliverySeq, shipmentManagement.notes)" result="flashScope.lastUpdateDate"/>
		</transition>
	</view-state>
</flow>