<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow
        http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd"
    parent="baseSearch">
    
    <view-state id="sectionPlacePopupSearch" view="src/sectionplacepopup/sectionPlacePopup_search"
       parent="baseSearch#search">
        <transition on="validate" validate="true">
            <set name="flowScope.isCurrentPageSearch" value="false" />
            <set name="flowScope.isShowList" value="true"></set>
        </transition>
        <transition on="back" to="home" validate="false" />
    </view-state>

    <subflow-state id="list" subflow="src/sectionPlacePopup/list" parent="baseSearch#list">
    </subflow-state>
    
</flow>