<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow
        http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd"
    parent="base">
    
    <input name="identify" />
    
    <on-start>
        <evaluate expression="SPMST050DeleteService.init(identify)"
            result="flowScope.userMasterManagement" />
    </on-start>
    
    <view-state id="SPMST050Delete"  view="mst/user/SPMST050_delete" model="identify">
        <transition on="delete" validate="true">
            <evaluate expression="SPMST050DeleteService.delete(identify)" />
            <set name="flashScope.isSuccess" value="true"></set>
        </transition>
    </view-state>

</flow>